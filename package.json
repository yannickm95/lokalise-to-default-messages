    "eslint": "^7.12.1",
    "husky": "^4.3.0",
    "prettier": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
